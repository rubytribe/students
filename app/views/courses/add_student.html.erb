<h3> Add a student </h3>
  
  <%= will_paginate @students %>
  
  <table>
    <% @students.each do |s| %>
      <tr>
        <td> <%= s.full_name %> </td>
        <td> <%= link_to 'Add', enrollments_path(student_id: s.id, course_id: @course.id), method: :post %> </td>
      </tr>
    <% end %>
  </table>
  
  <%= will_paginate @students %>
  
